{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "yarn start",
    "start": "NODE_OPTIONS=\"--max-old-space-size=8192\" docusaurus start",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "format": "prettier --write '(docs|guides|blog)/**/*.{js,jsx,ts,tsx,md,mdx}'",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate-snippets": "node ./scripts/generate-snippets.mjs && node  ./scripts/generate-snippets-solana.mjs",
    "generate-github-examples": "node scripts/fetch-github-example-repos.mjs",
    "generate-example-pages": "node scripts/make-example-pages.mjs",
    "generate-chain-docs": "npx ts-node --esm scripts/generate-chain-docs.ts",
    "make-docs:typescript": "node scripts/make-typescript-docs.mjs",
    "make-docs:react": "node scripts/make-react-docs.mjs",
    "make-docs:storage": "node scripts/make-storage-docs.mjs",
    "make-docs:solana": "node scripts/make-solana-docs.mjs",
    "make-docs:contracts": "node scripts/make-contracts-docs.mjs",
    "make-docs:python": "node scripts/make-python-docs.mjs",
    "make-docs:go": "node scripts/make-go-docs.mjs",
    "make-docs:unity": "node scripts/make-unity-docs.mjs",
    "make-docs": "yarn make-docs:typescript && yarn make-docs:storage && yarn make-docs:solana && yarn make-docs:contracts && yarn make-docs:python && yarn make-docs:go && yarn make-docs:unity",
    "prettier": "prettier --write '(docs|guides|blog)/**/*.{js,jsx,ts,tsx,md,mdx}'"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/plugin-content-docs": "^2.0.1",
    "@docusaurus/plugin-ideal-image": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.1",
    "@mdx-js/react": "^1.6.21",
    "@thirdweb-dev/react": "^3",
    "@thirdweb-dev/sdk": "^3",
    "clsx": "^1.1.1",
    "docusaurus-node-polyfills": "^1.0.0",
    "docusaurus-plugin-sass": "^0.2.2",
    "ethers": "5",
    "posthog-docusaurus": "^1.0.3",
    "posthog-js": "^1.20.2",
    "prism-react-renderer": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^3.1.1",
    "react-transition-group": "^4.4.5",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.1",
    "@swc/core": "^1.2.181",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/fs-extra": "^9.0.13",
    "docusaurus-plugin-module-alias": "^0.0.2",
    "fs-extra": "^10.0.1",
    "glob": "^8.0.1",
    "lokijs": "^1.5.12",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.5.1",
    "readline": "^1.3.0",
    "swc-loader": "^0.2.0",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
